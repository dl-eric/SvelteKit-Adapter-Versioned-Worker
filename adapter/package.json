{
	"name": "internal-adapter",
	"version": "1.0.0",
	"files": [
		"build",
		"static",
		"!static/tsconfig.json",
		"virtual-modules",
		"src",
		"index.ts"
	],
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./build/index.d.ts",
			"import": "./build/index.js"
		},
		"./worker": {
			"types": "./virtual-modules/worker.d.ts",
			"import": "./virtual-modules/worker.js"
		},
		"./worker/util": {
			"types": "./build/src/worker/util.d.ts",
			"import": "./build/src/worker/util.js"
		},
		"./runtime-constants": {
			"types": "./virtual-modules/runtime-constants.d.ts",
			"import": "./virtual-modules/runtime-constants.js"
		},
		"./internal/exported-by-svelte-module": {
			"types": "./build/src/exportedBySvelteModule.d.ts",
			"import": "./build/src/exportedBySvelteModule.js"
		}
	},
	"types": "./build/index.d.ts",
	"typesVersions": {
		"*": {
			"worker": ["./virtual-modules/worker.d.ts"],
			"worker/util": ["./build/src/worker/util.d.ts"],
			"runtime-constants": ["./virtual-modules/runtime-constants.d.ts"],
			"internal/exported-by-svelte-module": ["./build/src/exportedBySvelteModule.d.ts"]
		}
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"@types/web-app-manifest": "^1.0.4",
		
		"@rollup/plugin-alias": "^4.0.3",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-typescript": "^11.0.0",
		"@sveltejs/adapter-static": "^2.0.1",
		"kleur": "^4.1.5",
		"make-dir": "^4.0.0",
		"mime-types": "^2.1.35",
		"recursive-readdir": "^2.2.3",
		"rollup": "^3.19.1",
		"rollup-plugin-esbuild": "^5.0.0",
		"tslib": "^2.5.0",
		"typescript": "^5.0.2"
	},
	"devDependencies": {
		"@sveltejs/kit": "^1.19.0",
		"@types/mime-types": "^2.1.1",
		"@types/node": "^18.13.0",
		"@types/recursive-readdir": "^2.2.1",
		"vite": "^4.3.9"
	},
	"keywords": [],
	"author": "",
	"type": "module"
}
